{
  "host": "localhost",
  "port": 3030,
  "public": "../public/",
  "buildPath": "dist",
  "rootSrc": "src",
  "paginate": {
    "default": 10,
    "max": 50
  },
  "authentication": {
    "secret": "",
    "strategies": [
      "jwt",
      "local"
    ],
    "path": "/authentication",
    "service": "users",
    "jwt": {
      "header": {
        "type": "access"
      },
      "audience": "https://yourdomain.com",
      "subject": "anonymous",
      "issuer": "feathers",
      "algorithm": "HS256",
      "expiresIn": "1d"
    },
    "local": {
      "entity": "user",
      "usernameField": "email",
      "passwordField": "password"
    }
  },
  "db": {
    "dialect": "mysql",
    "connectionString": "mysql://root:@localhost:3306/inventory_server",
    "logging": true,
    "define": {
      "freezeTableName": true
    }
  },
  "logger": {
    "fileLevel": "debug",
    "consoleLevel": "debug",
    "directory": "logs"
  }
}
